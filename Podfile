    inherit! :search_paths
    # Pods for testing
  end

  target 'Dash TransportationUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  #Pods for Google Maps
  source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '11.1'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  
  

end
